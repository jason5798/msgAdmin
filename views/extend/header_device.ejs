<!DOCTYPE html>
<html>
<head>
  <meta charset="utf-8">
  <meta name="viewport" content="width=device-width, initial-scale=1">
  <title><%= title%></title>
  <%- include include_style.ejs %>

</head>

<body>
  <div id="wrapper">
        <nav class="navbar navbar-default navbar-cls-top " role="navigation" style="margin-bottom: 0">
            <div class="navbar-header">
                <button type="button" class="navbar-toggle" data-toggle="collapse" data-target=".sidebar-collapse">
                    <span class="sr-only">Toggle navigation</span>
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                </button>
                <% if(co == 'gemteks'){ %>
                <a class="navbar-brand" href="/tools">Device admin</a>
                <% } %>

            </div>

        </nav>
           <!-- /. NAV TOP  -->
        <nav class="navbar-default navbar-side" role="navigation">
          <div class="sidebar-collapse">
              <% if(co == 'blazing'){ %>
                <img src="/icons/ST2.png" class="center-block iconStyle"/>
              <% } %>
              <ul class="nav" id="main-menu">
              <% if(isNeedTypeSwitch){ %>
                  <li>
                  <% if(type==='pir'){ %>
                    <li>
                        <button id="pir" name="pir" class="center-block btnStyle" onclick="back()">
                        <img src="/icons/STI1n.png" class="btnIcon"/>
                        <label class="fa fa-2x" style="color:#ffffff">PIR&nbsp&nbsp&nbsp&nbsp&nbsp</label>
                        </button>

                  <% }else if(type==='gps'){%>

                        <button id="gps" name="gps" class="center-block btnStyle" onclick="back()">
                        <img src="/icons/STI2n.png" class="btnIcon"/>
                        <label class="fa fa-2x" style="color:#ffffff">Tracker</label>
                        </button>

                  <% }else if(type==='gpm25'){%>

                        <button id="pm25" name="pm25" class="center-block btnStyle" onclick="back()">
                        <img src="/icons/STI3n.png" class="btnIcon"/>
                        <label class="fa fa-2x" style="color:#ffffff">&nbspPM2.5&nbsp&nbsp</label>
                        </button>

                  <% }else if(type==='flood'){%>

                        <button id="flood" name="flood" class="center-block btnStyle" onclick="back()">
                        <img src="/icons/STI4n.png" class="btnIcon"/>
                        <label class="fa fa-2x" style="color:#ffffff">&nbspFlood&nbsp&nbsp</label>

                  <% }else if(type==='others'){%>

                        <button id="flood" name="flood" class="center-block btnStyle" onclick="back()">
                        <img src="/icons/STI5n.png" class="btnIcon"/>
                        <label class="fa fa-2x" style="color:#ffffff">&nbspOthers&nbsp&nbsp</label>


                  <% } %>
                  </li>
              <% }else{ %>
<!-- Function classification ------------------------------------->

                    <li  >
                        <% if(title ==='Device'){ %>
                          <a class="active-menu"  href="/">
                        <% }else{ %>
                          <a href="/">
                        <% } %>
                          <i class="fa fa-table fa-3x"></i>
                          <label class="fa fa-2x" style="color:#ffffff">Device</label>
                        </a>
                    </li>
              <% } %>
              </ul>

          </div>
        </nav>
        <!-- /. NAV SIDE  -->
        <div id="page-wrapper" >
          <div id="page-inner">
             <div class="row">

            <div class="col-md-12">
              <div >
                  <button type="button" id="back" name="back" class="btn btn-primary btn-sm" onClick="back()">
                        <img src="/icons/back.png" width="ˇ30" height="30"/> Back
                    </button>
                    <% if(co == 'blazing'){ %>
                      <img src="/icons/Logo1n.png" class="center-block logoStyle"/>
                    <% } %>
                    &nbsp<br>
            </div>

            <div class="col-md-12 column" style="margin-top: -10px;">

                        <h2>
                            <% if(type==='pir'){ %>
                            PIR:
                            <% }else if(type==='gps'){%>
                            Tracker:
                            <% }else if(type==='pm25'){%>
                            PM2.5:
                            <% }else if(type==='flood'){%>
                            Flood:
                            <% }else if(type==='others'){%>
                            Others:
                            <% }else {%>
                            Mac:
                            <% } %>
                        <%= mac%></h2>
                    </div>

                    <div class="col-md-12 column">